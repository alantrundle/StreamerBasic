{
  "name": "ESP32-WROVER N16R8 (16MB Flash, 8MB PSRAM)",
  "vendor": "Custom",
  "url": "https://docs.espressif.com/projects/esp-idf/en/latest/esp32/",
  "frameworks": ["arduino", "espidf"],
  "connectivity": ["wifi", "bluetooth"],

  "build": {
    "core": "esp32",
    "mcu": "esp32",
    "variant": "esp32",

    "f_cpu": "240000000L",
    "f_flash": "40000000L",
    "flash_mode": "dio",

    "flash_size": "16MB",
    "partitions": "large_spiffs_16MB.csv",

    "extra_flags": [
      "-DBOARD_HAS_PSRAM",
      "-mfix-esp32-psram-cache-issue"
    ]
  },

  "upload": {
    "flash_size": "16MB",
    "maximum_size": 4718592,
    "maximum_ram_size": 327680,
    "speed": 460800
  },

  "debug": {
    "openocd_board": "esp32-wrover-kit-3.3v.cfg"
  }
}